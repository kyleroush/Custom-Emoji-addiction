{

  "description": "When you need to have more then the normie emojis in your life.",
  "manifest_version": 2,
  "name": "Github Custom Emoji Addiction",
  "version": "1.5.2",
  "homepage_url": "https://github.com/kyleroush/Custom-Emoji-addiction",

  "icons": {
    "48": "logo.png"
  },

  "permissions": [
    "storage"
  ],

  "browser_action": {
    "default_title": "Github Custom Emoji Addiction Settings",
    "default_popup": "popup/Settings.html"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "jquery.js",
        "caret.js",
        "atwho.js",
        "GithubData.js",
        "script.js"
      ],
      "css" : [
        "atwho.css"
      ]
    }
  ]
}
